<% include includes/header %>
    
    <h1><%= title %></h1>

	<section>
    	<form enctype="multipart/form-data" action="/partners/submit" method="post">
	    	<h2>Add New Partner</h2>
	    	<input name="name" placeholder="Enter Title">
	    	<button type="submit">Submit</button>
	    </form>
    </section>
    <section>
    	<h2>Current Partners</h2>
    	<% for(var i =0; i<partners.length; i++){ %>
    		<form enctype="multipart/form-data" action="/partners/update" method="post">
    			<input type="text" name="name" value="<%= partners[i].name %>"/>
    			<input type="hidden" name="_id" value="<%= partners[i]._id %>"/>
    			<button type="submit">Update</button>
    			<button type="button" class="delete">Delete</button>
    		</form>
    	<% } %>
    </section>


<% include includes/footer %>